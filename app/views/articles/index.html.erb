<h1>Articles listing page </h1>
<table>
<thead>
<tr>
  <th>TITTLE</th>
   <th>DESCRIPTION</th>
    <th colspan="3">Action</th>
</tr>
</thead>
<tbody>
<% @articles.each do |article| %>
    <tr>
    <td><%=article.title%></td>
     <td><%=article.description%></td>
     <td><%=link_to 'show',article_path(article)%></td>
     <td><%=link_to 'Edit',edit_article_path(article)%></td>
      <td><%=link_to 'Delete', delete_article_path(article), method: :delete, data:{confirm:"Are you sure?"}%> </td>
    </tr>
    <%end%>

</tbody>
</table>
<p>
   <%=link_to 'create a new article',new_article_path%>
</p>
